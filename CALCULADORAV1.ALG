Algoritmo "calculadorav1"

Var
   numero1,numero2,soma:inteiro
   media:real
Inicio
      escreva("Digite o primeiro numero: ")
                      leia(numero1)
      escreva("Digite o segundo numero: ")
                      leia(numero2)

                      media<-(numero1+numero2)/3
      escreval("sua média: " ,media)


Fimalgoritmo